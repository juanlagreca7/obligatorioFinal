@model IEnumerable<AppVitalSignMonitor.Models.Reporte>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PresionDiastolica)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PresionSistolica)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SaturacionOxigeno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Temperatura)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pulso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HoraFecha)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PresionDiastolica)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PresionSistolica)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SaturacionOxigeno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Temperatura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pulso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HoraFecha)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdDispositivo">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdDispositivo">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdDispositivo">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
