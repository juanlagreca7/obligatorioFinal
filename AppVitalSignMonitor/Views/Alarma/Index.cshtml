@model IEnumerable<AppVitalSignMonitor.Models.Alarma>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ValorComparativo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoAlarma)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoComparacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaCreacion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ValorComparativo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoAlarma)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoComparacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaCreacion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdAlarma">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdAlarma">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdAlarma">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
